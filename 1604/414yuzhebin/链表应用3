void  solve(Linklist &L)
{
    p=L->next;
    q=p->next;
    r=q->next;
    while (q!=L)
    {
        while((p!=L)&&(p->data>q->data))
                p=p->back;
                q->back->next=r;
                r->back=q->back;
                q->next=p->next;
                q->back=p;
                p->next=q;
                r->back-back=q;
                q=r;
                p=q->back;
                r=L->next;
}
